						function checkFont(font) {
							var str = $("#generatorTextInput").val();
							var arr = str.split('');
							for(var i = 0; i < arr.length; i++) {
								if (arr[i].charCodeAt(0) == '32') {
									arr.splice(i, 1);
									i--;
								}
							}
							length = arr.length;
							if(length == 0) {
								length = $('#generatorResultTextSpan').text().length;
							}
							size = $('.sizePickerButton.pickerActive').data('size');
							width = fontsData[font]['length'][$('#generatorResultTextSpan').text().length][size]['width'];
							height = fontsData[font]['length'][$('#generatorResultTextSpan').text().length][size]['height'];
							price = fontsData[font]['length'][length][size]['sum'];
							$('#resultTextWidth').text(width);
							$('#resultTextHeight').text(height);
							$('#priceVal').text(price);
							$('#constr-weight').val(width);
							$('#constr-height').val(height);
							$('#constr-price').val(price);
						}

						//Изменение текста
						$("#generatorTextInput").keyup(function(e) {
							font = $('.fontPickerButton.pickerActive').data('font');
							currText = $(this).val(); //текущий текст
							formatText = currText.replace(new RegExp('\n', 'g'),"<br>"); //замена переносов строки на html
							if (formatText == '') formatText = 'Text'; //Если пустой текст
							$("#generatorResultTextSpan").html(formatText); //Подстановка текста
							setTextHeight(currText); //Установка высоты поля вывода
							setResultTextInfo(currText); //установка высоты вывески в меню
							if (currText == '') currText = 'Text'; //Если пустой текст
							//$("textarea.t330__input").val(currText); //добавление текста в форму
							$("#constr-text").val(currText);
							setFormValues();
							checkFont(font);
						});
		
