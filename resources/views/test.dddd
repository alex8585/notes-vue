						function setResultTextInfo(inputText) {
								if(fontsData[currFont] == null) {
									stringHeight = 6;
									letterWidth = 12;
								} else {
									stringHeight = fontsData[currFont]['stringHeight'];
									letterWidth = fontsData[currFont]['letterWidth'];
								}
								letterPrice = 1900; //цена за букву
								minPrice = 6900;
								currBreaks = (inputText.match(new RegExp("\n", "g")) || []).length; //Вычисление количества переносов строк
								currHeight = (currBreaks + 1) * stringHeight; //вычисление новой высоты
								currSplit = inputText.split("\n"); //разбивка строк на массив
								longestString = currSplit.sort(function (a, b) { return b.length - a.length; })[0]; //нахождение самой длинной строки
								currWidth = longestString.length * letterWidth; //вычисление новой ширины
								if (currWidth == 0) currWidth = 9 * letterWidth; //подстановка плейсхолдера
								if (currWidth != currInputWidth) {
										scale = currInputWidth/currWidth;
										newFontSize = Math.round(32 * scale);
										if (newFontSize >= 220) {
											newFontSize = 220;
										}
										if ($(window).width() < 480 && newFontSize >= 150) {
											newFontSize = 150;
										}
										currOutputWidth = currInputWidth;
										currOutputHeight = Math.round(stringHeight * scale);
										$("#generatorResultText").css("font-size",newFontSize + "px");
								} else {
										$("#generatorResultText").css("font-size","32px");
										currBreaks = (inputText.match(new RegExp("\n", "g")) || []).length; //Вычисление количества переносов строк
										currOutputHeight = (currBreaks + 1) * stringHeight; //вычисление новой высоты
										currOutputWidth = longestString.length * letterWidth; //вычисление новой ширины
										if (currOutputWidth == 0) currOutputWidth = 9 * letterWidth; //подстановка плейсхолдера
								}
								$("#resultTextHeight").html(currOutputHeight); //установка высоты вывески в меню
								$("#resultTextWidth").html(currOutputWidth); //установка ширины вывески в меню
								$("#inputWidth").val(currOutputWidth);
								$("#inputHeight").val(currOutputHeight);
								currBreakReplace = inputText.replace(new RegExp('\n', 'g'),""); //удаление переноса строк для рассчета
								currBreakReplace = currBreakReplace.replace(new RegExp(' ', 'g'),""); //удаление пробелов для рассчета
								newPrice = currBreakReplace.length * letterPrice; //рассчет цены
								newPrice = newPrice + minPrice;
								if (currPanel != 'None') {
										if (currPanelType != 'Square') newPrice += 1250;
										newPrice += 2900;
								}
								if (newPrice == 0) newPrice = minPrice; //установка плейсхолдера
								priceConvertDelim = 1; //место разбики цены для вывода
								if (newPrice >= 10000) priceConvertDelim = 2; //место разбики цены для вывода если цена больше 10к
								if (newPrice >= 100000) priceConvertDelim = 3; //место разбики цены для вывода если цена больше 100к
								newPriceText = newPrice.toString().substr(0,priceConvertDelim) + ' ' + newPrice.toString().substr(priceConvertDelim); //разбивка цены пробелом
								$("#generatorPriceNumber").html(newPriceText); //подстановка новой цены
						}
				});
				//Вычисление и установка размеров контейнера
				function resizeContainer() {
						currScreenWidth = $("#generatorContainer").parent("div").width(); //ширина контейнера
						if (currScreenWidth > 1200) currScreenWidth = 1200; //установка максимальной ширины
						currScreenHeight = (currScreenWidth * 0.6743055555555556); //вычисление ширины
						$("#generatorBg, #generatorContainer").height(currScreenHeight); //установка высоты контейнера
						$("#generatorBg, #generatorContainer").width(currScreenWidth); //установка высоты контейнера
				}
				//Вычисление и установка размеров меню
				function resizeControls() {
						currScreenWidth = $(window).width();
						if (currScreenWidth > 780) {
								currMenuHeight = $("#generatorBg").height() - 30; //вычисление ширины
								$("#generatorControls").height(currMenuHeight); //установка высоты контейнера
